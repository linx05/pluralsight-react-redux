{
  "name": "pluralsight-redux-starter",
  "version": "1.0.0",
  "description": "Starter kit for React and Redux Pluralsight course by Cory House",
  "author": "Cesar Laredo",
  "license": "MIT",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "babel-node tools/srcServer.js",
    "dev": "npm-run-all --parallel start test:watch",
    "lint": "esw webpack.config.* src tools --watch",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "dependencies": {
    "babel-polyfill": "6.26.0",
    "bootstrap": "3.3.7",
    "jquery": "3.2.1",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "4.2.2",
    "react-router-redux": "4.0.8",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.26.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "compression": "1.7.1",
    "cross-env": "5.1.1",
    "css-loader": "0.28.7",
    "enzyme": "3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "4.10.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-react": "7.4.0",
    "eslint-watch": "3.1.3",
    "eventsource-polyfill": "0.9.6",
    "expect": "21.2.1",
    "express": "4.16.2",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "0.11.2",
    "jsdom": "8.5.0",
    "mocha": "3.5.3",
    "nock": "9.0.27",
    "npm-run-all": "4.1.1",
    "open": "0.0.5",
    "react-test-renderer": "^16.0.0",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-mock-store": "1.3.0",
    "rimraf": "2.6.2",
    "style-loader": "0.19.0",
    "url-loader": "0.5.9",
    "webpack": "3.8.1",
    "webpack-dev-middleware": "1.12.0",
    "webpack-hot-middleware": "2.20.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/linx05/pluralsight-react-redux"
  }
}
